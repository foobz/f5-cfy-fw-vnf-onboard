#!/bin/sh
unset -v line1 line2
{ IFS= read -r line1 && IFS= read -r line2;} < name

if [ $line1 = "install" ]; then
 /usr/bin/cfy profiles use 192.168.109.43 -u admin -p passw0rd -t default_tenant
 /usr/bin/cfy deployment create $line1 -b f5-cfy-fw-vnf-onboard
 /usr/bin/cfy executions start -d $line1 install
elif [ $line1 = "uninstall" ]; then
 /usr/bin/cfy executions start -d $line1 uninstall
 /usr/bin/cfy deployments delete $line1
fi
